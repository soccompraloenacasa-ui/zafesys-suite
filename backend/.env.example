# ZAFESYS Suite - Environment Variables
# Copy this file to .env and fill in your values

# Database (Railway provides this automatically)
DATABASE_URL=postgresql://user:password@host:5432/database

# JWT Secret (generate a strong random string)
SECRET_KEY=your-super-secret-key-change-in-production-use-32-chars-min

# CORS Origins (comma-separated list of allowed origins)
# For development: *
# For production: https://yourdomain.com,https://www.yourdomain.com
CORS_ORIGINS=*

# ElevenLabs Configuration
ELEVENLABS_WEBHOOK_SECRET=your-elevenlabs-webhook-secret
ELEVENLABS_AGENT_ID=your-agent-id

# Optional
DEBUG=false
